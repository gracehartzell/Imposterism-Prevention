
<% let questions = quiz.questions %>
<div>
<% questions.forEach((question) => { %>
    <%= question.content %>
            <code style="border: solid fuchsia;"><%=question.code_snippet %></code>
        
            <div style="border: dashed greenyellow">
                <p class="answerContent">

                <button class="answerChoice">A</button>
            <%= JSON.stringify(question.choices[0].text) %>
            </p>
            <p class="answerContent">
                <button class="answerChoice">B</button>
                <%= JSON.stringify(question.choices[1].text) %>
            </p>
            <p class="answerContent">
                <button class="answerChoice">C</button>
                <%= JSON.stringify(question.choices[2].text) %>
            </p>
            <p class="answerContent">
                <button class="answerChoice">D</button>
                <%= JSON.stringify(question.choices[3].text) %>
            </p>
            <p class="answerContent">
                <button class="answerChoice">E</button>
                <%= JSON.stringify(question.choices[4].text) %>
            </p>
        
        </div>
        <% }); %>

    </div>
    
